{
	"name": "client",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"generate-masks": "TS_NODE_PROJECT=tsconfig.scripts.json node --loader ts-node/esm scripts/generateMaskSizes.ts",
		"prerender": "tsx --tsconfig tsconfig.app.json prerender/prerender.tsx",
		"sitemap": "tsx --tsconfig tsconfig.app.json scripts/generateSitemap.ts",
		"build": "npm run generate-masks && cross-env NODE_ENV=production npm run sitemap && tsc -b && vite build",
		"postbuild": "cross-env NODE_ENV=production npm run prerender",
		"preview": "vite preview"
	},
	"dependencies": {
		"@dnd-kit/core": "^6.3.1",
		"@dnd-kit/sortable": "^10.0.0",
		"@fontsource/archivo": "^5.2.6",
		"@fontsource/inter": "^5.2.6",
		"@fontsource/nunito": "^5.2.6",
		"@fontsource/source-serif-4": "^5.2.8",
		"@fortawesome/fontawesome-svg-core": "^7.0.1",
		"@fortawesome/free-brands-svg-icons": "^7.0.1",
		"@fortawesome/free-regular-svg-icons": "^7.0.1",
		"@fortawesome/free-solid-svg-icons": "^7.0.1",
		"@fortawesome/react-fontawesome": "^3.0.2",
		"@tailwindcss/vite": "^4.1.13",
		"axios": "^1.12.2",
		"clsx": "^2.1.1",
		"cross-env": "^10.1.0",
		"dotenv": "^17.2.3",
		"framer-motion": "^12.23.13",
		"react": "^19.1.1",
		"react-dom": "^19.1.1",
		"react-router-dom": "^7.8.2",
		"sitemap": "^9.0.0",
		"tailwindcss": "^4.1.13"
	},
	"devDependencies": {
		"@eslint/js": "^9.33.0",
		"@types/node": "^24.9.2",
		"@types/react": "^19.2.4",
		"@types/react-dom": "^19.1.9",
		"@types/react-router-dom": "^5.3.3",
		"@vitejs/plugin-react-swc": "^4.0.0",
		"eslint": "^9.33.0",
		"eslint-plugin-react-hooks": "^5.2.0",
		"eslint-plugin-react-refresh": "^0.4.20",
		"globals": "^16.3.0",
		"ts-node": "^10.9.2",
		"tsx": "^4.20.6",
		"typescript": "~5.8.3",
		"typescript-eslint": "^8.39.1",
		"vite": "^7.1.2"
	}
}
